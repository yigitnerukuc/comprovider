{
  "name": "yigitnerukuc/comprovider",
  "type": "composer-plugin",
  "description": "Comprovider is a composer plugin which provides to initialize automatically installed Laravel packages via Composer",
  "keywords": ["comprovider", "yigitnerukuc"],
  "homepage": "http://github.com/yigitnerukuc/comprovider",
  "license" : "BSD",
  "authors" : [
    {
      "name": "Yigit Cukuren",
      "email": "yigit@yigit.io",
      "homepage": "http://www.yigit.io"
    }
  ],
  "require": {
    "composer-plugin-api": "^1.0"

  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^5.0"
  },
  "autoload": {
    "psr-4": {
      "ComProvider\\": "src/"
    }
  },
  "extra": {
    "class": "ComProvider\\ComProvider"
  }
}
